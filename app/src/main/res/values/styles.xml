<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!--定义主题，解决白屏 -->
    <style name="LaunchTheme" parent="AppTheme">
        <item name="android:windowBackground">@color/colorPrimary</item>
    </style>

    <!--分割线-->
    <style name="line">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/colorLine</item>
        <item name="android:layout_marginTop">@dimen/marginSize</item>
    </style>
    <!--登录按钮-->
    <style name="btnCommit">
        <item name="android:layout_height">@dimen/btn_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@drawable/btn_commit_select</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">@dimen/marginSize</item>
    </style>
</resources>
